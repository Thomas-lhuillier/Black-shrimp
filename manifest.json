{
// "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Black-shrimp",
  "description": "desc",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "Thomas L'huillier",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+T"
      }
    }
  },
  "background": {
    "scripts": ["contentscript.js"],
    "persistent": false
  },
  "web_accessible_resources": ["*.woff"],
  "browser_action": {
    "default_icon": {
      "16": "assets/img/icon16.png"
    },
    "default_title": "Toolkit"
  },
  "icons": { 
    "16":  "assets/img/icon16.png",
    "32":  "assets/img/icon16@2x.png",
    "48":  "assets/img/icon48.png",
    "128": "assets/img/icon128.png" 
  },
  "permissions": [
    "activeTab"
  ]
}